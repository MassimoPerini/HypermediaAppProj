<script>
    $(document).ready(function(){
        $('.main-landmarks li.dropdown').hover(function() {
            $(this).find('ul').stop(true, true).delay(200).fadeIn(150);
        }, function() {
            $(this).find('ul').stop(true, true).delay(200).fadeOut(150);
        });
    });
</script>

