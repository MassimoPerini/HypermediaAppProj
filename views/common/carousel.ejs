<div id="main-slider" class="carousel slide" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#main-slider" data-slide-to="0" class="active"></li>

      <%for (i=1;i<imgs.length;i++){%>
      <li data-target="#main-slider" data-slide-to="<%=i%>"></li>
      <%}%>
  </ol>

  <!-- Wrapper for slides -->
  <div class="carousel-inner">
    <div class="item active">
      <img src="<%=imgs[0]%>" alt="<%=imgs[0]%>">
    </div>

    <%for (i=1;i<imgs.length;i++){%>
        <div class="item">
            <img src="<%=imgs[i]%>" alt="<%=imgs[i]%>">
        </div>
      <%}%>

  </div>

  <!-- Left and right controls -->
  <a class="left carousel-control" href="#main-slider" data-slide="prev">
    <span class="glyphicon glyphicon-chevron-left"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="#main-slider" data-slide="next">
    <span class="glyphicon glyphicon-chevron-right"></span>
    <span class="sr-only">Next</span>
  </a>
</div>